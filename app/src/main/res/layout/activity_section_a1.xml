                                        style="@style/checkbox"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="1dp"
                                        android:text="@string/na119g" />

                                    <CheckBox
                                        android:id="@+id/na11996"
                                        style="@style/checkbox"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="1dp"
                                        android:text="@string/other" />

                                </LinearLayout>

                                <EditText
                                    android:id="@+id/na11996x"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZ abcdefghijklmnopqrstuvwxyz"
                                    android:hint="@string/other"
                                    android:text='@{na11996.checked ? na11996x.getText.toString : ""}'
                                    android:visibility='@{na11996.checked ? View.VISIBLE : View.GONE}' />

                            </LinearLayout>

                        </LinearLayout>

                        <!--  = = = = =  END OF FORM FIELDS  = = = = = -->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="end"
                            android:layout_marginTop="80dp"
                            android:orientation="horizontal">

                            <Button
                                android:id="@+id/btn_End"
                                style="@style/buttons"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/red"
                                android:onClick="@{() -> callback.BtnEnd()}"
                                android:text="@string/end_interview"
                                android:textColor="@color/white" />

                            <Button
                                android:id="@+id/btn_Continue"
                                style="@style/buttons"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="#1ed61e"
                                android:onClick="@{() -> callback.BtnContinue()}"
                                android:text="@string/nextSection"
                                android:textColor="@color/white"
                                android:visibility="@{na11801b.checked ? View.GONE : View.VISIBLE}" />

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </ScrollView>
    </LinearLayout>
</layout>
